#include <stdio.h>

int main()
{
    float marks1, marks2, marks3, marks4, marks5, average;

    printf("Enter grade from subject 1:");
    scanf("%f", &marks1);
    printf("Enter grade from subject 2:");
    scanf("%f", &marks2);
    printf("Enter grade from subject 3:");
    scanf("%f", &marks3);
    printf("Enter grade from subject 4:");
    scanf("%f", &marks4);
    printf("Enter grade from subject 5:");
    scanf("%f", &marks5);

    average = (marks1 + marks2 + marks3 + marks4 + marks5) / 5;
    printf("Average : %0.2f\n", average);

    if (average >= 90)
    {
        printf("Your Grade is A");
    }
    else if (average >= 80)
    {
        printf("Your Grade is B");
    }
    else if (average >= 70)
    {
        printf("Your Grade is C");
    }
    else if (average >= 60)
    {
        printf("Your Grade is D");
    }
    else
    {
        printf("Your Grade is F");
    }

    return 0;
}
